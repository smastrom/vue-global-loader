{
   "compilerOptions": {
      "target": "ES2018",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "lib": ["ESNext", "DOM"],
      "skipLibCheck": true,
      "jsx": "preserve",
      "jsxImportSource": "vue",
      "noEmit": true,
      "baseUrl": ".",
      "paths": {
         "@/support/*": ["./cypress/support/*"],
         "@/*": ["./*"]
      }
   },
   "include": ["."]
}
