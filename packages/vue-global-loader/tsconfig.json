{
   "compilerOptions": {
      "target": "ES2015",
      "module": "ES2020",
      "moduleResolution": "Node",
      "strict": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "lib": ["ES2015", "DOM"],
      "skipLibCheck": true,
      "noEmit": true,
      "allowJs": true,
      "checkJs": true
   },
   "include": ["."]
}
